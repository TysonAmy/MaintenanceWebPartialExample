@model IEnumerable<MaintenanceWebsite.Models.AppUser>
@{
    ViewData["Title"] = "Index";
}

<h1>List of Users</h1>
<a asp-action="Create">Create</a>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
            </tr>

        }
    </tbody>
</table>  